<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.10.3 -->
<interface>
  <!-- interface-name interface.ui -->
  <requires lib="gtk" version="4.6"/>
  <template class="snarvei_window" parent="GtkApplicationWindow">
    <property name="child">
      <object class="GtkBox" id="master_box">
        <child>
          <object class="GtkListBox" id="menu_list">
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkListBox" id="macro_list">
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
          </object>
        </child>
      </object>
    </property>
    <property name="title">Snarvei</property>
    <child type="titlebar">
      <object class="GtkHeaderBar">
        <child type="end">
          <object class="GtkMenuButton" id="menu_button"/>
        </child>
        <child>
          <object class="GtkButton" id="new_collection_button">
            <property name="label">New</property>
            <signal name="clicked" handler="handle_new_collection" swapped="yes"/>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
